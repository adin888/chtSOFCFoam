U
{
    internalField   uniform (0 0 0);

    boundaryField
    {
        ".*"
        {
            type            fixedValue;
            value           uniform (0 0 0);
        }
        fuelInlet
        {
            type            fixedValue;
            value           uniform (1.0 0 0);
        }
        fuelOutlet
        {
            type            zeroGradient;
        }
    }
}

T
{
    internalField   uniform 1000;

    boundaryField
    {
        ".*"
        {
            type            zeroGradient;
        }

        fuelInlet
        {
            type            fixedValue;
            value           uniform 1000;
        }
        fuelOutlet
        {
            type            zeroGradient;
        }

        "fuel_to_.*"
        {
            type            compressible::turbulentTemperatureRadCoupledMixed;
            Tnbr            T;
            kappaMethod     fluidThermo;
            value           uniform 1000;
        }
    }
}

