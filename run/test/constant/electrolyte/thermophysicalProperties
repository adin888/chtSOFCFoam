{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight   	50;
        nElectrons      0;
        rSign		0;
        hForm		0;
        sForm		0;
    }
    transport
    {
        kappa   24;
    }
    thermodynamics
    {
        Hf      0;
        Cp      550;
    }
    equationOfState
    {
        rho     6000;
    }
}


